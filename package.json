{
  "name": "slate-starter-vue-theme",
  "version": "1.0.0-alpha.1",
  "private": true,
  "author": "Shopify Inc.",
  "description": "An opinionated, Slate compatible, starting point for developing Shopify themes.",
  "keywords": [
    "shopify",
    "theme"
  ],
  "bugs": {
    "url": "https://github.com/Earthlandto/slate-starter-vue-theme/issues"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Earthlandto/slate-starter-vue-theme.git"
  },
  "devDependencies": {
    "@shopify/slate-tools": "1.0.0-beta.14",
    "babel-preset-shopify": "^16.2.0",
    "copy-webpack-plugin": "^4.6.0",
    "eslint-plugin-shopify": "^19.0.0",
    "fs": "0.0.1-security",
    "node-sass": "^4.11.0",
    "prettier": "^1.16.1",
    "sass-loader": "^7.1.0",
    "stylelint-config-prettier": "^3.0.4",
    "stylelint-config-shopify": "^5.0.0",
    "vue": "^2.5.22",
    "vue-loader": "^15.5.1",
    "vue-template-compiler": "^2.5.22",
    "webpack-sources": "^1.3.0"
  },
  "dependencies": {
    "@shopify/theme-a11y": "^1.0.1",
    "@shopify/theme-addresses": "^1.0.0",
    "@shopify/theme-cart": "^1.0.0-alpha.7",
    "@shopify/theme-currency": "^1.0.0-alpha.7",
    "@shopify/theme-images": "^1.0.0-alpha.7",
    "@shopify/theme-rte": "^1.0.0-alpha.7",
    "@shopify/theme-sections": "^1.1.0",
    "@shopify/theme-variants": "^1.0.0-alpha.8",
    "jquery": "^3.2.1",
    "lazysizes": "^4.1.1",
    "lodash-es": "^4.17.4",
    "normalize.css": "^7.0.0",
    "qrcode": "^1.3.2"
  },
  "scripts": {
    "start": "slate-tools start",
    "watch": "slate-tools start --skipFirstDeploy",
    "build": "slate-tools build",
    "deploy": "npm run build && slate-tools deploy",
    "deploy:replace": "npm run build && slate-tools deploy --replace",
    "zip": "npm run build && slate-tools zip",
    "lint": "slate-tools lint",
    "format": "slate-tools format",
    "update-cert": "chmod +x ./ssl-check.sh && ./ssl-check.sh",
    "dev": "npm run watch",
    "dev:deploy": "npm run start"
  },
  "homepage": "https://github.com/Shopify/starter-theme#readme",
  "main": "slate.config.js"
}
